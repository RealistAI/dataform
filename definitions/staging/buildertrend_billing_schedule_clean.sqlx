config { type: "table"}

SELECT
	LOWER(TRIM(REGEXP_EXTRACT(job_name, r"\(([a-z A-Z]*)\)*"))) as model,
	job_name,
	title as scheduled_item,
	completed,
	scheduled_end,
	assigned_to
FROM
	${ref("schedule_clean")}
WHERE
	job_name NOT LIKE "%Test%"
AND
	job_name NOT LIKE "%TEST%"
AND
	job_name != "Quote-John & Mary Smith (Barcelona Plat)"
